{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CompTIA SY0-701 Objectives",
  "type": "object",
  "additionalProperties": false,
  "required": ["exam_code", "version", "source_pdf", "generated_at", "domains", "objectives"],
  "properties": {
    "exam_code": { "type": "string", "const": "SY0-701" },
    "version": { "type": "string" },
    "source_pdf": { "type": "string" },
    "generated_at": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}T" },
    "domains": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "title"],
        "properties": {
          "id": { "type": "string", "pattern": "^\\d+\\.0$" },
          "title": { "type": "string", "minLength": 1 }
        }
      }
    },
    "objectives": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "title", "domain_id"],
        "properties": {
          "id": { "type": "string", "pattern": "^\\d+\\.\\d+$" },
          "title": { "type": "string", "minLength": 1 },
          "domain_id": { "type": "string", "pattern": "^\\d+\\.0$" }
        }
      }
    }
  }
}
